762. Prime Number of Set Bits in Binary Representation


class Solution {
    public int countPrimeSetBits(int left, int right) {
        int ans=0;
        for(int i=left; i<=right; i++){
            int setBits= Integer.bitCount(i);
            if(isPrime(setBits)==true) ans=ans+1;
        }
        return ans;
    }
    static boolean isPrime(int n)
    {
        if (n <= 1)
            return false;
        for (int i = 2; i < n; i++)
            if (n % i == 0)
                return false;
        return true;
    }
}


class Solution {
    public int countPrimeSetBits(int left, int right) {
        int ans=0;
        for(int i=left; i<=right; i++){
            int setBits= countOnes(i);
            if(isPrime(setBits)==true) ans=ans+1;
        }
        return ans;
    }
    static boolean isPrime(int n)
    {
        if (n <= 1)
            return false;
        for (int i = 2; i < n; i++)
            if (n % i == 0)
                return false;
        return true;
    }
    private int countOnes(int n) {
    int count = 0;
    while (n > 0) {
        count += (n & 1);
        n >>= 1;          
    }
    return count;
}
}